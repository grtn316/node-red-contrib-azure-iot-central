  "dependencies": {
    "azure-iot-device-mqtt": "*",
    "azure-iot-device-amqp": "*",
    "azure-iot-device-http": "*",
    "azure-iot-device": "*",
    "azure-iot-provisioning-device-mqtt": "*",
    "azure-iot-security-symmetric-key": "*",
    "azure-iot-security-x509": "*",
    "azure-iot-provisioning-device": "*"
  },
  "keywords": [
    "node-red",
    "Azure",
    "IoT",
    "IoT Central"
  ],
  "author": "Pietro Brambati",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pietrobr/node-red-contrib-azure-iot-central.git"
  },
  "node-red": {
    "nodes": {
      "red-iot-central": "red-iot-central.js"
    }
  }
}
